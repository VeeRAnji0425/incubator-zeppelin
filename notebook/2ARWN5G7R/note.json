    {
      "title": "Solr in action",
      "text": "CassandraConnector(sc.getConf).withSessionDo { \n    session \u003d\u003e {\n        session.execute(s\"\"\"\n        SELECT year,performer,title \n        FROM spark_demo.albums \n        WHERE solr_query\u003d\u0027performer:\"*${z.input(\"Performer\",\"Jackson\")}*\" \n        AND title:\"*${z.input(\"Title\",\"\")}*\" \n        AND year:${z.input(\"Year\",\"\")}\u0027\n        \"\"\").\n        all().\n        toSeq.\n        map(row \u003d\u003e (row.getInt(\"year\"),row.getString(\"performer\"),row.getString(\"title\"))).\n        sortBy{ case(year,performer,title)\u003d\u003e (year,performer,title)}.\n        displayAsTable(\"Performer\",\"Title\",\"Year\")\n    }\n}",
